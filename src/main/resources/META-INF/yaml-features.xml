<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <applicationService
                serviceInterface="io.codiga.plugins.jetbrains.services.Rosie"
                serviceImplementation="io.codiga.plugins.jetbrains.services.RosieImpl"
                testServiceImplementation="io.codiga.plugins.jetbrains.services.RosieTest"
        />
        <postStartupActivity implementation="io.codiga.plugins.jetbrains.starter.RosieRulesCacheUpdater"/>
        <externalAnnotator
                language=""
                implementationClass="io.codiga.plugins.jetbrains.annotators.RosieAnnotator"/>

        <applicationService
                serviceImplementation="io.codiga.plugins.jetbrains.actions.snippet_search.service.YamlFileTypeService"/>
    </extensions>

    <extensions defaultExtensionNs="JavaScript.JsonSchema">
        <ProviderFactory implementation="io.codiga.plugins.jetbrains.services.CodigaConfigSchemaProviderFactory"/>
    </extensions>
</idea-plugin>
